@model entityLayer.concrete.about
@{
    ViewData["Title"] = "editAbout";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>editAbout</h1>

<h1>Update About</h1>

@using (Html.BeginForm("editAbout", "adminAbout", FormMethod.Post))
{
    <div class=" form-group">
        <label>About Value</label>
        @Html.TextBoxFor(i=>i.aboutValue , new {@class="form-control"})
        <br>
        <label>About Image Path</label>
        @Html.TextBoxFor(i=>i.aboutImagePath , new {@class="form-control"})

        @Html.HiddenFor(i=>i.aboutId)

        <br>
        <button class="btn btn-primary">Update About</button>
    </div>
}
